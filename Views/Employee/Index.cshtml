@using EmployeeManegmentSystem.Models;
@model IEnumerable< EmployeeManegmentSystem.Models.Employee>;



<h1>@ViewBag.PageDetails</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id </th>
            <th>Name </th>
            <th> Email</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Department</td>
            </tr>
        }

    </tbody>
</table>


